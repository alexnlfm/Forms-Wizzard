{
   "extends": ["eslint:recommended", "plugin:react/recommended"],
   "plugins": [],
   "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
   },
   "rules": {
      "react/prop-types": "off"
   }
}
